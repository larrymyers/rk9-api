  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run server",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "cmd/rk9/main.go",
      "args": ["-server"],
      "cwd": "${workspaceFolder}",
      "envFile": "${workspaceFolder}/development.env"
    },
    {
      "name": "Run loader",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "cmd/rk9-loader/main.go",
      "cwd": "${workspaceFolder}",
      "envFile": "${workspaceFolder}/development.env"
    }
  ]
}
